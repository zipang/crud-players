{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "todos",
	"type": "object",
	"title": "JSON Schema for Players entries",
	"description": "A player in a game team",
	"required": [
		"firstName",
		"lastName",
		"number",
		"team"
	],
	"properties": {
		"firstName": {
			"type": "string"
		},
		"lastName": {
			"type": "string"
		},
		"birthDate": {
			"type": "string",
			"format": "date",
			"description": "Born on"
		},
		"number": {
			"type": "integer",
			"description": "Official number in the team"
		},
		"team": {
			"type": "string"
		},
		"biography": {
			"type": "string"
		}
	},
	"examples": [
		{
			"firstName": "John",
			"lastName": "DOE",
			"biography": "John DOE was allways here. Even before the Game was created.",
			"number": 10,
			"team": "X"
		},
		{
			"firstName": "Arthur",
			"lastName": "ASH",
			"biography": "Arthur was never were we expected him to be. I think he played alone the whole time.",
			"number": 0,
			"team": "Roland Garros"
		}
	]
}
